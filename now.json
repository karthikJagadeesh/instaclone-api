{
  "name": "instaclone-api",
  "alias": "instaclone-api",
  "version": 2,
  "builds": [{ "src": "/api/app.js", "use": "@now/node-server" }],
  "routes": [{ "src": "^/(.*)", "dest": "/api/app.js" }],
  "env": {
    "MONGODB_URI": "@instaclone-mongodb-uri",
    "PORT": "@instaclone-port",
    "SECRET": "@instaclone-secret"
  },
  "github": {
    "silent": true
  }
}
